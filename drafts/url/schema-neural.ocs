union activation {
   Tanh { };
   Sigmoid { };
   Linear { };
   ReLU { };
}

union data {
   Circle { };
   Xor { };
   Gauss { };
   Spiral { };
}

block urlParams {
   .activation: activation

   [-- Eventually I want to figure out how to make this >=1 and <=30 --]
   .batchSize:  int

   .dataSet:    data
}

block urlBase {
   .base:       string
}

block neural {
   .urlBase:    urlBase
   .urlParams:  urlParams
}

root {
   .neural:  neural
}
