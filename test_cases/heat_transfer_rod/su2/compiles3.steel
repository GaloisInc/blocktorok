config {
  timeDomain: transient; -- steady state or transient simulation
  timeStep : 0.005 s;-- size of the time step
  timeIterations : 200.0; -- total number of time steps
  const Temp = 273.0 K; -- SOLID_TEMPERATURE_INIT
  run: m1(Temp);
  backend : Su2 {
      format : LIB_InputOutput,
  };
}

model m1(qw) {
  technique : FVM;
  innerIterations: 200;
  mesh: mesh_solid_rod.su2;
  boundaryField : LIB_BoundaryCondition;
  physics : HeatConduction{LIB_SolvingTechnique};
  const k = 318.0 W/(m*K);
  const c = 130.0 J/(kg*K);
  const rho = 19300.0 kg/m^3;
  dT/dt - k/(c*rho)*â–³T = 0;
  solve Tw with {LIB_NumericalScheme, LIB_PhysicsParameters};
  return Tw;
}
