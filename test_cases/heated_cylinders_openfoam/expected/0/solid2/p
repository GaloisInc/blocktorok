FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

// calculated: the property you are referring to is already fully defined by the other boundary conditions and properties you defined in the constant folder

boundaryField
{
    ".*"
    {
        type            calculated;
        value           $internalField;
    }
}

