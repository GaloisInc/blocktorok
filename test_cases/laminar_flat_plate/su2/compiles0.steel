config {
  timeDomain: steady; -- steady state or transient simulation
  timeIterations : 9999; -- total number of time steps
  run: m1();
  backend : Su2 {
      format : LIB_InputOutput,
      grid : LIB_MultiGrid
  };
}

model m1(x) {
  technique : FVM;
  mesh: mesh_flatplate_65x65.su2;
  boundaryField : LIB_BoundaryCondition;
  physics : FluidFlow{LIB_SolvingTechnique};
  solve y with {LIB_NumericalScheme, LIB_PhysicsParameters};
  return y;
}
